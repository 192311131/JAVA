class Box {
    double length;
    double width;
    double height;

    Box() {
        length = 1.0;
        width = 1.0;
        height = 1.0;
    }

    double calculateVolume() {
        return length * width * height;
    }
}

public class Main {
    public static void main(String[] args) {
        Box myBox = new Box();
        double volume = myBox.calculateVolume();
        System.out.println("Volume of the Box: " + volume);
    }
}
